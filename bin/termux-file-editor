#!/usr/bin/env -S bash -l

# NOTE: This should really live in ~/local/lib/linux/termux/common/bin,
#       but the Termux activities that invoke it expect it to live in
#       ~/bin

if [[ "$FLAVOR" == "termux" ]]; then
	true # Currently this is a no-op
else
	echo "This script should only be run in Termux"
	exit 1
fi
